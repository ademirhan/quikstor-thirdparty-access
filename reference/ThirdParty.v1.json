      "post": {
        "summary": "login post",
        "tags": [
          "login"
        ],
        "responses": {
          "200": {
            "description": "API responses with the TokenResponse model(See models.)",
            "content": {
              "application/json": {
                "schema": {
                  "type": "object",
                  "properties": {
                    "access_token": {
                      "type": "string"
                    },
                    "token_type": {
                      "type": "string"
                    },
                    "expires_in": {
                      "type": "string",
                      "format": "date-time"
                    },
                    "refresh_token": {
                      "type": "string"
                    }
                  }
                }
              },
              "application/xml": {
                "schema": {
                  "type": "object",
                  "properties": {}
                }
              }
            }
          }
        },
        "operationId": "post-login",
        "requestBody": {
          "content": {
            "text/plain": {
              "schema": {
                "type": "object",
                "properties": {
                  "grant-type": {
                    "type": "string"
                  },
                  "username": {
                    "type": "string"
                  },
                  "password": {
                    "type": "string"
                  }
                },
                "required": [
                  "grant-type",
                  "username",
                  "password"
                ]
              },
              "examples": {
                "example-1": {
                  "value": "grant_type=password&username=<third-party-user-name>@quikstor.com&password=<third-party-password>"
                }
              }
            }
          },
          "description": "Raw text body required for authentication"
        },
        "description": "Get an access token from API test",
        "security": []
      }
    },
